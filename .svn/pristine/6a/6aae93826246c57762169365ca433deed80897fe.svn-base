body, html {
  background-color: #f7f7f7;
}
/*头部*/
.header{
  position: relative;
  width: 100%;
  height: 44px;
  font-size: 20px;
  font-weight: bold;
  text-align: center;
  line-height: 44px;
  color: #fff;
  background-color: #e4393c;
  >.back{
    z-index: 100;
    width: 50px;
    height: 44px;
    background:url("data:image/svg+xml;charset=utf-8,<svg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 1024 1024' width='1.5em' height='1.5em'><path fill='#FFFFFF' d='M818.881422 747.988622 755.734756 812 313.503289 363.943111 313.480533 363.965867 250.311111 299.931733 755.734756-212.113778 818.881422-148.1024 376.672711 299.954489Z' transform='translate(0, 812) scale(1, -1)'/></svg>") no-repeat;
    background-position: 10px 10px;
  }
  > .home{
    position: absolute;
    width: 50px;
    height: 44px;
    background: url("../../imgs/baicaijia/header_app.png") no-repeat;
    -webkit-background-size: 50px 44px;
    background-size: 50px 44px;
    right: 0;
    top:0;
  }
}


/*标题列表*/
.bcj-head-cat{
  position: relative;
  height: 38px;
  display: -webkit-box;
  border-bottom: 1px solid #eeeeee;
  .tabs {
    background-color: #FFFFFF;
    border-bottom: none;
    width: 100%;
    overflow: hidden;
    display: -webkit-box;
    -webkit-box-flex: 1;
    ul{
      width: 100%;
      li.active, li.hover {
        background: transparent;
        a {
          color: #e4393c;
          border-bottom: 2px solid #e4393c;
        }
      }
      li {
        float: left;
        height: 38px;
        font-size: 14px;
        line-height: 36px;
        a {
          color: #333333;
          display: inline-block;
          font-weight: normal;
          text-decoration: none;
          text-align: center;
          overflow: hidden;
          padding: 0;
          margin: 0 12px;
          width: auto;
        }
      }
    }
  }
  /*预备类*/
  .arrow {
    width: 38px;
    height: 38px;
    border-left: 1px solid #e6e6e6;
    background-color: #fbfbfb;
    display: -webkit-box;
    .icon-arrow {
      background: url("data:image/svg+xml;charset=utf-8,<svg viewBox='0 0 60 120' xmlns='http://www.w3.org/2000/svg'><path d='m60 61.5-38.25 38.25-9.75-9.75 29.25-28.5-29.25-28.5 9.75-9.75z' fill='%23c7c7cc'/></svg>");
      width: 38px;
      height: 38px;
      background-repeat: no-repeat;
      background-position: center center;
      background-size: 12px auto;
    }
  }
  /*搜索*/
