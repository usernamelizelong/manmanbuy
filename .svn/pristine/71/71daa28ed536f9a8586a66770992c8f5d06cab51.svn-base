/*��������ͼ����ʽ*/
.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
}
/*ҳ������*/
.mm-layout {
  width: 100%;
  background-color: #fff;
  /*ͷ����ʽ*/
  /*�̳ǵ����б���ʽ*/
}
.mm-layout > header {
  width: 100%;
  height: 44px;
  line-height: 44px;
  background-color: #ff841d;
}
.mm-layout > header > .h-back {
  float: left;
}
.mm-layout > header > .h-back > a {
  display: block;
  width: 50px;
  text-align: center;
  color: #fff;
}
.mm-layout > header > .h-back > a a:hover {
  text-decoration: none;
}
.mm-layout > header > .h-back > a ::before {
  content: '';
  font-weight: bold;
}
.mm-layout > header > .h-app {
  float: right;
  padding-top: 4px;
}
.mm-layout > header > .h-app > a {
  display: block;
  width: 50px;
  height: 40px;
  background: url("../../images/sitenav/header_app.png") no-repeat;
  background-size: 30px 30px ;
  line-height: 40px;
}
.mm-layout > header > span {
  display: block;
  width: 100%;
  font-size: 20px;
  text-align: center;
  font-weight: bold;
  color: #fff;
}
.mm-layout > .list {
  width: 100%;
}
.mm-layout > .list > .list-t {
  float: left;
  width: 40% ;
  height: 45px;
  text-align: center;
  margin: 10px 5% 0;
}
.mm-layout > .list > .list-t > a {
  text-decoration: none;
  display: inline-block;
  background-color: red;
  width: 100%;
  height: 45px;
  line-height: 45px;
  background-color: #f4f4f4;
  border: 1px dashed #ccc;
}
.mm-layout > .list > .list-t > a > img {
  margin-top: -5px;
  display: inline;
}
.mm-layout > .list > .list-t > a > span {
  margin-left: 5px;
}
.mm-layout > .bottom {
  width: 100%;
  height: 36px;
  background-color: #f7f7f7;
  border-top: 1px solid #ccc;
  border-bottom: 1px solid #ccc;
  margin: 20px 0;
  line-height: 36px;
  color: #000;
  padding-left: 10px;
}
.mm-layout > #app {
  text-decoration: none;
  position: fixed;
  width: 68px;
  height: 68px;
  right: 0;
  bottom: 30px;
  background: url("../../images/sitenav/appxiazai.png") no-repeat;
  background-size: contain  ;
}
