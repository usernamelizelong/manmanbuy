.v_layout {
  height: 100%;
  width: 100%;
  margin: 0 auto;
  background-color: #F2F2F2;
}
.v_layout > form {
  height: 100%;
}
.v_layout > form .v_nav {
  height: 40px;
  border-top: 2px solid #EBEBEB;
  border-bottom: 1px solid #CCCCCC;
}
.v_layout > form .v_nav > a {
  float: left;
  display: block;
  height: 40px;
  line-height: 40px;
}
.v_layout > form .v_nav > a:first-of-type {
  padding: 0 2px 0 5px;
  line-height: 40px;
}
.v_layout > form .v_nav > a:nth-of-type(2) {
  padding: 0 2px 0 5px;
}
.v_layout > form .v_good > .v_good_header {
  height: 36px;
  background-color: #F5F5F5;
  border-bottom: 1px solid #CCCCCC;
  border-top: 1px solid #CCCCCC;
}
.v_layout > form .v_good > .v_good_header > p {
  width: 170px;
  line-height: 36px;
  padding-left: 10px;
  font-size: 16px;
  color: #874604;
}
.v_layout > form .v_good > .v_good_list ul:first-of-type em {
  background-color: #F10E0E;
}
.v_layout > form .v_good > .v_good_list li {
  margin-top: 10px;
  height: 56px;
  background: url("data:image/svg+xml;charset=utf-8,<svg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 1024 1024'><path fill='#999999' d='M746.255255 299.653099 746.33098 299.728824 713.12674 332.933063 713.049992 332.857339 297.859875 748.048479 264.635169 714.82275 679.816077 299.642866 264.635169-115.538041 297.859875-148.762747 713.00906 266.386438 713.085808 266.30969 746.341213 299.567141Z'  transform='translate(0, 812) scale(1, -1)'/></svg>") right center no-repeat;
  background-size: 20px 20px;
  border-bottom: 1px solid #E7E7E7;
}
.v_layout > form .v_good > .v_good_list li > a {
  position: relative;
  display: block;
  height: 45px;
  margin-left: 10px;
}
.v_layout > form .v_good > .v_good_list li > a em {
  position: absolute;
  top: 0;
  left: 0;
  width: 20px;
  height: 20px;
  text-align: center;
  background-color: #C9C9C9;
  color: #fff;
}
.v_layout > form .v_good > .v_good_list li > a > .inner {
  width: 150px;
  margin-left: 30px;
}
.v_layout > form .v_good > .v_good_list li > a > .inner > div {
  font-size: 16px;
  color: #000;
}
.v_layout > form .v_good > .v_good_list li > a > .inner > p {
  margin-top: 6px;
}
.v_layout > form .v_good > .v_good_list ul:nth-of-type(2) em {
  background-color: #FF9314;
}
.v_layout > form .v_good > .v_good_list ul:nth-of-type(3) em {
  background-color: #8ADF5B;
}
.v_layout > form .v_sale_header {
  height: 36px;
  border-bottom: 1px solid #E7E7E7;
}
.v_layout > form .v_sale_header > h3 {
  height: 46px;
  padding-left: 10px;
}
.v_layout > form .v_sale_header > h3 > a {
  float: left;
  line-height: 36px;
  color: #874604;
  font-size: 14px;
}
.v_layout > form .v_sale_menu {
  background-color: #F2F2F2;
}
.v_layout > form .v_sale_menu .m_main {
  padding-left: 120px ;
  padding-top: 10px;
  position: relative;
}
.v_layout > form .v_sale_menu .m_main .m_l {
  position: absolute;
  left: 10px;
  top: 10px;
}
.v_layout > form .v_sale_menu .m_main .m_font {
  font-size: 14px;
  margin-bottom: 36px;
}
.v_layout > form .v_sale_menu .m_main .m_price em {
  font-size: 16px;
  width: 90px;
  text-align: center;
}
.v_layout > form .v_sale_menu .m_main .m_price .star {
  float: right;
  margin-right: 10px;
  display: inline-block;
  width: 100px;
  height: 20px;
  background: url("data:image/svg+xml;charset=utf-8,<svg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 1024 1024'><path fill='#ff9000' d='m496.52887,736.87439l112.46838,-346.20175l364.01178,0l-294.48053,-213.97614l112.4809,-346.2005l-294.48053,213.96616l-294.53922,-213.96616l112.48215,346.2005l-294.4818,213.97614l364.01427,0l112.5246,346.20175z'  transform='translate(0, 812) scale(1, -1)'/></svg>") right center repeat;
  color: red;
}
.v_layout > form .v_sale_menu .m_main .other {
  padding-top: 13px;
}
.v_layout > form .v_comment_header {
  height: 36px;
  border-bottom: 1px solid #E7E7E7;
}
.v_layout > form .v_comment_header > h3 {
  height: 46px;
  padding-left: 10px;
}
.v_layout > form .v_comment_header > h3 > a {
  float: left;
  line-height: 36px;
  color: #874604;
  font-size: 14px;
}
.v_layout > form .v_comment_menu ul li {
  padding-bottom: 12px;
}
.v_layout > form .v_comment_menu ul li .m_t {
  position: relative;
  padding-left: 120px ;
  padding-top: 10px;
}
.v_layout > form .v_comment_menu ul li .m_t .m_img {
  position: absolute;
  left: 10px;
  top: 10px;
}
.v_layout > form .v_comment_menu ul li .m_b {
  position: relative;
  margin-top: 80px;
  margin-left: 6px;
  background-color: #F8F8F8;
  border: 1px solid #E7E7E7 ;
}
.v_layout > form .v_comment_menu ul li .m_b:after {
  content: "";
  position: absolute;
  border-bottom: 8px solid #f8f8f8;
  border-left: 7px solid transparent;
  border-right: 7px solid transparent;
  top: -7px;
  left: 30px;
}
.v_layout > form .v_comment_menu ul li .m_b .m_x {
  margin-left: 11px;
  margin-top: 11px;
  height: 26px;
}
.v_layout > form .v_comment_menu ul li .m_b .m_x .star {
  display: inline-block;
  width: 60px;
  height: 15px;
  background: url("data:image/svg+xml;charset=utf-8,<svg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 1024 1024'><path fill='#ff9000' d='m496.52887,736.87439l112.46838,-346.20175l364.01178,0l-294.48053,-213.97614l112.4809,-346.2005l-294.48053,213.96616l-294.53922,-213.96616l112.48215,346.2005l-294.4818,213.97614l364.01427,0l112.5246,346.20175z'  transform='translate(0, 812) scale(1, -1)'/></svg>") right center repeat;
}
.v_layout > form .v_comment_menu ul li .m_b .m_x .m_date {
  float: right;
  padding-right: 4px;
}
.v_layout > form .v_comment_menu ul li .m_b .c_send {
  margin: 5px 10px;
}
a.gotop {
  position: fixed;
  display: none;
  width: 40px;
  height: 40px;
  z-index: 999;
  right: 8px;
  bottom: 30px;
  background: #000 url("data:image/svg+xml;charset=utf-8,<svg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 1024 1024' width='17' height='17'><path fill='#565656' d='M888.890368 434.863104 524.079104 69.685248c-3.62496-3.62496-8.512512-5.63712-13.600768-5.63712-5.08928 0-9.976832 2.01216-13.601792 5.63712L132.06528 434.863104c-16.028672 16.04608-15.031296 42.750976 0.9984 58.76224 15.962112 15.962112 41.76896 16.096256 57.831424 0.050176l277.981184-277.998592 0 604.1344c0 22.694912 18.390016 40.886272 40.971264 40.886272 22.612992 0 42.233856-18.19136 42.233856-40.886272L552.081408 215.676928l277.983232 277.998592c16.06144 16.045056 41.867264 15.911936 57.8304-0.050176C903.921664 477.613056 904.91904 450.909184 888.890368 434.863104z' transform='translate(0, 0)'/></svg>") no-repeat;
  border-radius: 20px;
  background-position: center center;
  opacity: 0.3;
}
.overlay {
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  z-index: 9999;
  display: block;
  background-color: rgba(0, 0, 0, 0.3);
}
.overlay img {
  position: absolute;
  left: 50%;
  top: 50%;
  width: 100px;
  margin: -80px 0 0 -50px;
}
