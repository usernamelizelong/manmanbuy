  background-repeat: no-repeat;
  color: #ffffff;
  background-size: 22px 22px;
  background-position: center center;
}
.gsshop-section > .option-n > div.active > i {
  background-image: url("data:image/svg+xml;charset=utf-8,<svg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 1024 1024' width='17' height='17'><path fill='#565656' d='M573.989475 312.942837l288.898365 279.702021c14.022629 13.587739 14.022629 35.617094 0 49.206846-14.020551 13.587739-36.757056 13.587739-50.780724 0L523.185891 362.128539 234.265705 641.852712c-14.023668 13.587739-36.760173 13.587739-50.780724 0-14.022629-13.589753-14.022629-35.620114 0-49.206846l288.898365-279.702021L183.484981 33.240816c-14.022629-13.587739-14.022629-35.617094 0-49.206846 14.020551-13.587739 36.757056-13.587739 50.780724 0l288.920186 279.724172 288.920186-279.724172c14.023668-13.587739 36.760173-13.587739 50.780724 0 14.022629 13.589753 14.022629 35.620114 0 49.206846L573.989475 312.942837z' transform='translate(0, 812) scale(1, -1)'/></svg>");
}
.gsshop-section > .option-n > ul {
  height: 40px;
  width: 100%;
  background: #fff;
  border-bottom: 1px solid #dadada;
  padding-right: 40px;
}
.gsshop-section > .option-n > ul > li {
  float: left;
  width: 33.3%;
  height: 40px;
  line-height: 40px;
  font-size: 16px;
  text-align: center;
}
.gsshop-section > .option-n > ul > li > a {
  display: inline-block;
  height: 40px;
  width: 100%;
}
.gsshop-section > .option-n > ul > li > a > i {
  display: inline-block;
  width: 18px;
  height: 18px;
  vertical-align: middle;
  background-image: url("data:image/svg+xml;charset=utf-8,<svg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 1024 1024' width='17' height='17'><path fill='#565656' d='M778.88 445.386667 528.106667 194.72 277.44 445.386667Z' transform='translate(0, 812) scale(1, -1)'/></svg>");
}
.gsshop-section > .option-n > ul > li.active > a > i {
  background-image: url("data:image/svg+xml;charset=utf-8,<svg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 1024 1024' width='17' height='17'><path fill='#565656' d='M277.44 194.72 528.213333 445.386667 778.88 194.72Z' transform='translate(0, 812) scale(1, -1)'/></svg>");
}
.gsshop-section > .option-f {
  position: relative;
}
.gsshop-section > .option-f > ul {
  display: none;
  position: absolute;
  top: 0px;
  left: 0px;
  width: 100%;
}
.gsshop-section > .option-f > ul > li {
  background-color: #fff;
  width: 100%;
  height: 40px;
  line-height: 40px;
  font-size: 16px;
  color: #333333;
  border-bottom: 1px solid #dadada;
}
.gsshop-section > .option-f > ul > li > a {
  width: 100%;
  text-indent: 1em;
  display: inline-block;
  height: 40px;
}
.gsshop-section > .option-f > .choice-box {
  display: none;
  position: absolute;
  top: 0px;
  left: 0px;
  width: 100%;
  padding: 10px;
  background-color: #f7f7f7;
  box-shadow: 0px 3px 6px #dddddd;
}
.gsshop-section > .option-f > .choice-box .search {
  padding: 0px 50px 0px 0px;
}
.gsshop-section > .option-f > .choice-box .search input:nth-of-type(1) {
  width: 100%;
  height: 35px;
  padding-left: 25px;
  border: 1px solid #EEEEEE;
}
.gsshop-section > .option-f > .choice-box .search input:nth-last-of-type(1) {
  position: absolute;
  right: 10px;
  top: 10px;
  height: 35px;
  width: 30px;
  background-color: transparent;
  border: transparent;
  color: #000;
  font-size: 14px;
}
.gsshop-section > .option-f > .choice-box .search > i {
  display: block;
  position: absolute;
  width: 35px;
  height: 35px;
  left: 10px;
  top: 10px;
  padding: 10px;
  background-origin: padding-box;
  background-clip: content-box;
  background-size: 16px 16px;
  background-position: center center;
  background-image: url("data:image/svg+xml;charset=utf-8,<svg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 1024 1024' width='16' height='16'><path fill='#a9a9a9' d='M978.58816-132.04608 710.3488 136.22144c53.91872 64.8384 86.37312 148.11904 86.37312 239.01184 0 206.6304-167.48032 374.112-374.10944 374.112-206.59328 0-374.11328-167.4816-374.11328-374.112 0-206.592 167.52-374.11456 374.11328-374.11456 90.912 0 174.17088 32.45824 239.01056 86.37696l268.24192-268.27008L978.58816-132.04608zM87.87968 375.23328c0 184.57472 150.13632 334.73152 334.7328 334.73152 184.55552 0 334.73152-150.1568 334.73152-334.73152 0-184.57728-150.176-334.7328-334.73152-334.7328C238.01728 40.50048 87.87968 190.656 87.87968 375.23328z' transform='translate(0, 812) scale(1, -1)'/></svg>");
}
.gsshop-section > .option-f > .choice-box > p {
  margin-top: 20px;
  margin-bottom: 10px;
  color: #000;
  font-size: 14px;
}
.gsshop-section > .option-f > .choice-box > div dl {
  width: 100%;
  height: 32px;
}
.gsshop-section > .option-f > .choice-box > div dl > dd {
  float: left;
  width: 75px;
  height: 30px;
  margin: 2px;
  color: #8b8b8b;
  background-color: #fff;
  text-align: center;
  line-height: 30px;
  font-size: 14px;
  border-radius: 4px;
}
.gsshop-section > .option-f > .choice-box > div dl > dd.active {
  background-color: #ff9501;
  color: #fff;
}
/*商品列表*/
.gsshop-commodity > ul {
  background-color: #f1f1f1;
}
.gsshop-commodity > ul > li {
  width: 50%;
  padding: 5px;
  float: left;
}
.gsshop-commodity > ul > li > a {
  display: block;
  width: 100%;
  height: 100%;
  background-color: #fff;
}
.gsshop-commodity > ul > li > a > .pic {
  width: 100%;
  height: auto;
  text-align: center;
  overflow: hidden;
}
.gsshop-commodity > ul > li > a > .pic > img {
  max-width: 100%;
  max-height: 100%;
}
.gsshop-commodity > ul > li > a > div:nth-last-of-type(1) {
  color: #000;
  overflow: hidden;
  height: 14px;
  line-height: 14px;
  font-size: 12px;
  margin-top: 10px;
  text-indent: 5px;
}
.gsshop-commodity > ul > li > a > p {
  font-size: 20px;
  color: #f8601d;
  margin-bottom: 10px;
  margin-top: 10px;
  text-indent: 5px;
}
.gsshop-commodity > ul > li > a > p > i {
  font-size: 12px;
  font-style: normal;
  color: #f8601d;
}
.gsshop-commodity > ul > li:nth-child(odd) {
  padding-left: 10px;
}
.gsshop-commodity > ul > li:nth-child(even) {
  padding-right: 10px;
}
