/*头部*/
.gsshop-header {
  width: 100%;
  background-color: #ff8b00;
  height: 44px;
  position: relative;
  box-shadow: 0px 2px 3px #dddddd;
}
.gsshop-header > div:first-child {
  height: 44px;
  position: absolute;
  left: 5px;
  top: 5px;
}
.gsshop-header > div:first-child > a {
  width: 18px;
  height: 18px;
  font-size: 26px;
  font-family: serif;
  font-weight: bold;
  margin-left: 10px;
  color: #fff;
  margin-top: 10px;
  display: block;
}
.gsshop-header > h1 {
  width: 100%;
  margin: 0;
  height: 44px;
  line-height: 44px;
  float: left;
  color: #fff;
  font-size: 20px;
  font-weight: normal;
  text-align: center;
  padding: 0px 62px;
}
.gsshop-header > div:last-child {
  position: absolute;
  right: 0px;
  width: 40px;
  height: 44px;
  line-height: 44px;
  font-size: 26px;
}
/*选项*/
.gsshop-section {
  position: relative;
  border-bottom: 1px solid #eeeeee;
  width: 100%;
  /*下拉*/
}
.gsshop-section > .option-n {
  width: 100%;
  position: relative;
  overflow: hidden;
}
.gsshop-section > .option-n > div {
  position: absolute;
  width: 40px;
  height: 40px;
  right: 0px;
  top: 0px;
  border-left: 1px solid #dadada;
}
