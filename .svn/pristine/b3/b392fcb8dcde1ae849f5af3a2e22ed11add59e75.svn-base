@font-face {
  font-family: "iconfont";
  src: url('/lib/font_ry3110rxuuaexw29/iconfont.eot?t=1488853392341'); /* IE9*/
  src: url('/lib/font_ry3110rxuuaexw29/iconfont.eot?t=1488853392341#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('/lib/font_ry3110rxuuaexw29/iconfont.woff?t=1488853392341') format('woff'), /* chrome, firefox */
  url('/lib/font_ry3110rxuuaexw29/iconfont.ttf?t=1488853392341') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+*/
  url('/lib/font_ry3110rxuuaexw29/iconfont.svg?t=1488853392341#iconfont') format('svg'); /* iOS 4.1- */
}

body,html{
  overflow-x: hidden;
}
.mm_layout{
  width: 100%;
  background-color: #f5f5f5;
  position: relative;
  .mm_header{
    width: 100%;
    height: 50px;
    background-color: #fd831d;
    position: relative;
    .mm_logo{
      width: 671px;
      height: 30px;
      display: block;
      background-image: url("../images/header_logo.png");      background-repeat: no-repeat;
      background-size: 230px 31px;
      position: absolute;
      top: 11px;
      left: 20px;
    }
  }
  .mm_app{
    width: 38px;
    height: 42px;
    display: block;
    background-image: url("../images/header_app.png");    background-repeat: no-repeat;
    background-size: 40px 40px;
    position: absolute;
    top: 5px;
    right: 15px;
  }
//  ����������
  .mm_search{
    width: 100%;
    height: 50px;
    background-color: #fdfdfd;
    padding: 10px;
    position: relative;
    > form{
      width: 94%;
      height: 100%;
      padding-right: 10px;
      >input:first-of-type{
        width: 95%;
        height: 30px;
        margin-right: 0;
        padding-left: 50px;
        border-radius: 100px;
        background-color: #efefef;
        border: none;
        font-size: 16px;
        color: #000;
      }
      >input:last-of-type{
        width: 50px;
        height: 30px;
        border: none;
        border-radius: 100px;
        background-color: #fd831d;
        color: #fff;
        font-size: 16px;
        position: absolute;
        top: 10px;
        right: 5px;
      }
    }
  }
// 菜单栏的样式��
  .mm_menu{
    width: 100%;
    background-color: #f5f5f5;
    margin-top: 10px;
    padding: 20px;
    border-top: 1px solid #ccc;
    .row{
      > div:nth-last-of-type(-n+4){
        display: none;
      }
      > div{
        padding: 0;
          > a{
            width: 100%;
            display: block;
            text-align: center;
            &:hover{
              text-decoration: none;
            }
            > img{
              width: 50px;
              margin: 0 auto;
            }
            > p{
              line-height: 30px;
              font-size: 14px;
            }
          }
      }
    }
  }
//  折扣栏的标题
  .mm_discount_title{
    width: 100%;
    height: 40px;
    background-color: #ff9000;
    background:-webkit-gradient(linear,0 0,0 100%,from(#ffaf47),to(#ff9000));
    border-bottom: 1px solid #ffc881;
    margin-top: 10px;
    .discount_box{
      width: 100%;
      height: 100%;
      > a{
        width: 100%;
        height: 100%;
        display: block;
      }
      .discount_title_left{
        float: left;
        font-size: 20px;
        font-family: 'SimSun';
        font-weight: bold;
        color: #fff;
        line-height: 40px;
      }
      .discount_title_right{
        font-family: iconfont;
        color: #fff;
        font-size: 20px;
        position: relative;
        &::before{
          content: '\e687';
          position: absolute;
          top: 10px;
          right: 15px;
        }
      }
    }
  }
//  主体每一个商品的产品
  .mm_product{
    width: 100%;
    background-color: #fdfdfd;
      .row{
        width: 100%;
        max-width: 960px;
        min-width: 320px;
        height: 100%;
        //background-color: pink;
        margin: 0 auto;
        > ul{
          width: 100%;
          li{
            width: 100%;
            height: 100%;
            padding: 14px 8px;
            border-bottom: 1px solid #ccc;
            > a{
              width: 100%;
              height: 100%;
              display: block;
              &:hover{
                text-decoration: none;
              }
              > img{
                width: 100px;
                height: 100px;
                margin-right: 10px;
                float: left;
              }
              .mm_product-right{
                width: 100%;
                  >h3{
                    color: #272727;
                    font-size: 17px;
                    > span{
                      color: #fd6c01;
                    }
                  }
                  .product-bottom{
                    padding-top: 40px;
                    > p{
                      float: left;
                    }
                    .Message-prompt{
                      float: right;
                      .icon-comment{
                        width: 18px;
                        height: 15px;
                        float: left;
                        margin-top: 2px;
                        margin-right: 5px;
                        background-image: url("data:image/svg+xml;charset=utf-8,<svg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 1024 1024' width='17'><path fill='#a1a1a1' d='M924.504962 664.898711 97.819886 664.898711c-17.557888 0-31.795147-14.241352-31.795147-31.795147l0-508.732583c0-17.557888 14.238282-31.792077 31.795147-31.792077l241.198387 0 149.664671-149.661601c6.209422-6.212492 14.34266-9.31618 22.478967-9.31618s16.269545 3.102664 22.479991 9.31618L683.306574 92.57788l241.198387 0c17.557888 0 31.795147 14.235213 31.795147 31.792077L956.300109 633.10254C956.300109 650.657358 942.061826 664.898711 924.504962 664.898711zM892.708791 156.169197l-222.569098 0c-8.428973 0-16.518209-3.354398-22.478967-9.31618l-136.498814-136.494721-136.498814 136.494721c-5.961782 5.961782-14.051017 9.31618-22.479991 9.31618l-222.569098 0L129.61401 601.304324l763.093758 0L892.707768 156.169197zM288.592814 378.735226m-63.591317 0a62.143 62.143 0 1 0 127.182634 0 62.143 62.143 0 1 0-127.182634 0ZM511.161912 378.735226m-63.591317 0a62.143 62.143 0 1 0 127.182634 0 62.143 62.143 0 1 0-127.182634 0ZM733.73101 378.735226m-63.591317 0a62.143 62.143 0 1 0 127.182634 0 62.143 62.143 0 1 0-127.182634 0Z' transform='translate(0, 812) scale(1, -1)'/></svg>");
                      }
                      > span{
                        font-size: 12px;
                      }
                    }
                  }
              }
            }
          }
      }
    }
  }
//  更多的优惠信息
  .mm_discount{
    width: 98%;
    height: 30px;
    background-color: #f8f8f8;
    margin: 10px auto;
    text-align: center;
    line-height: 30px;
    border-radius: 5px;
    border: 1px solid #dcdcdc;
    > a{
      width: 100%;
      height: 100%;
      display: block;
      &:hover{
        text-decoration: none;
      }
    }
  }
//  品牌排行
  .mm_brand_title{
    width: 100%;
    height: 40px;
    background-color: #ff9000;
    background:-webkit-gradient(linear,0 0,0 100%,from(#ffaf47),to(#ff9000));
    border-bottom: 1px solid #ffc881;
    margin-top: 10px;
    .brand_box{
      width: 100%;
      height: 100%;
      > a{
        width: 100%;
        height: 100%;
        display: block;
      }
      .brand_title_left{
        float: left;
        font-size: 20px;
        font-family: 'SimSun';
        font-weight: bold;
        color: #fff;
        line-height: 40px;
      }
      .brand_title_right{
        font-family: iconfont;
        color: #fff;
        font-size: 20px;
        position: relative;
        &::before{
          content: '\e687';
          position: absolute;
          top: 10px;
          right: 15px;
        }
      }
    }
  }
  .download_tab{
    width: 80px;
    height: 80px;
    position: fixed;
    right: 0;
    bottom: 10px;
    > a{
      width: 100%;
      height: 100%;
      display: block;
      > img{
        width: 70px;
        height: 70px;

      }
    }
  }
}

