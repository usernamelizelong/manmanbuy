.mm_search {
  width: 100%;
  height: 50px;
  background-color: #fdfdfd;
  padding: 10px;
  position: relative;
}
.mm_search > form {
  width: 94%;
  height: 100%;
  padding-right: 10px;
}
.mm_search > form > input:first-of-type {
  width: 95%;
  height: 30px;
  margin-right: 0;
  padding-left: 25px;
  border-radius: 100px;
  background-color: #efefef;
  color: #000;
  border: none;
  font-size: 18px;
}
.mm_search > form > input:last-of-type {
  width: 50px;
  height: 30px;
  border: none;
  border-radius: 100px;
  background-color: #fd831d;
  color: #fff;
  font-size: 16px;
  position: absolute;
  top: 10px;
  right: 5px;
}
