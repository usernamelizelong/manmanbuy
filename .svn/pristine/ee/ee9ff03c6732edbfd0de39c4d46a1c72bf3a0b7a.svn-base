@font-face {
  font-family: 'iconfont';
  src: url('../../lib/font_960y6yj4lwyiudi/iconfont.eot');
  src: url('../../lib/font_960y6yj4lwyiudi/iconfont.eot?#iefix') format('embedded-opentype'), url('../../lib/font_960y6yj4lwyiudi/iconfont.woff') format('woff'), url('../../lib/font_960y6yj4lwyiudi/iconfont.ttf') format('truetype'), url('../../lib/font_960y6yj4lwyiudi/iconfont.svg#iconfont') format('svg');
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
}
li {
  list-style: none;
}
.coupon-header {
  height: 70px;
  width: 100%;
  position: relative;
  background: #f4483e;
  margin-bottom: 20px;
}
.coupon-header .coupon-header-back {
  float: left;
  width: 50px;
  text-align: center;
  padding-left: 10px;
}
.coupon-header .coupon-header-back .iconfont {
  color: #fff;
  width: 100%;
  line-height: 70px;
  margin: 0 auto;
  left: 10px;
  background: no-repeat center;
  position: relative;
  font-size: 17px;
}
.coupon-header .coupon-header-back .iconfont ::before {
  content: "";
  font-size: 17px;
  position: absolute;
  top: 0;
  left: 0;
  font-family: "iconfont" !important;
}
.coupon-header h1 {
  width: 122px;
  height: 70px;
  line-height: 70px;
  font-size: 20px;
  margin: 0 auto;
  text-align: center;
  font-weight: 700;
  color: #fff;
}
.coupon-header .coupon-header-downLoad {
  float: right;
  width: 50px;
  text-align: center;
  padding-left: 10px;
}
.coupon-header .coupon-header-downLoad .coupon-header_app {
  display: block;
  top: -68px;
  width: 100%;
  height: 70px;
  line-height: 70px;
  right: 20px;
  background: url("../../images/header_app.png") no-repeat center;
  position: relative;
  background-size: 40px;
}
.coupon-main {
  width: 100%;
  height: 100px;
  border-bottom: 1px solid #c4c4c4;
  padding: 0px;
}
.coupon-main .coupon-main-l li a {
  display: inline-block;
  height: 100%;
  width: 100%;
}
.coupon-main .coupon-main-l li a .coupon-main-pic {
  width: 120px;
  height: 100px;
  overflow: hidden;
  float: left;
}
.coupon-main .coupon-main-l li a .coupon-main-pic img {
  max-width: 100px;
}
.coupon-main .coupon-main-l li a .coupon-main-info {
  padding-left: 10px;
}
.coupon-main .coupon-main-l li a .coupon-main-info .coupon-info-tit {
  font-weight: 700;
  color: #333333;
}
.coupon-main .coupon-main-l li a .coupon-main-info .coupon-info-tit h3 {
  height: 8px;
  line-height: 23px;
  font-size: 17px;
  text-align: justify;
  font-weight: normal;
  word-break: break-all;
  margin-top: 0;
  margin-bottom: 0;
}
.coupon-main .coupon-main-l li a .coupon-main-info .coupon-info-tit h2 {
  height: 12px;
  line-height: 24px;
  font-size: 15px;
}
.coupon-main .coupon-main-l li a .coupon-main-info .coupon-info-data {
  height: 40px;
  line-height: 42px;
  color: #ee7700;
}
.coupon-main .coupon-main-r {
  position: relative;
}
.coupon-main .coupon-main-r a {
  display: block;
  width: 20px;
  height: 25px;
  background: url("../../images/coupon/arr-right.jpg") no-repeat #fff;
  position: absolute;
  background-size: 20px;
  color: #fff;
  top: 30px;
  left: 0px;
}
.coupon-msg {
  display: block;
  width: 100%;
  height: 60px;
  line-height: 60px;
  font-size: 15px;
  margin-top: 20px;
  background: #f5f5f5;
  padding-left: 8px;
  border: 1px solid #c4c4c4;
  color: #000000;
}
.coupon-right-app {
  display: block;
  width: 60px;
  height: 68px;
  background: url("../../images/coupon/right-app.png") no-repeat;
  background-size: 40px;
  padding-right: 20px;
  vertical-align: middle;
}
